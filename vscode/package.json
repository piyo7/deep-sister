{
    "name": "dreamist",
    "displayName": "DreaMist",
    "description": "どりーみすと",
    "version": "0.0.1",
    "publisher": "夢霧もろは",
    "engines": {
        "vscode": "^1.10.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "dreamist.switchIcons",
                "title": "Switch Icons"
            },
            {
                "command": "dreamist.downloadIcons",
                "title": "Download Icons"
            },
            {
                "command": "dreamist.rubyNovel",
                "title": "Ruby Novel"
            },
            {
                "command": "dreamist.publishNovel",
                "title": "Publish Novel"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.2.1",
        "vscode": "^1.0.5",
        "@types/node": "^7.0.5"
    }
}